<?xml version="1.0" encoding="utf-8"?>
<!--btn_capture.xml-->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item>
    <shape
      android:shape="oval"
      android:thickness="3dp">
      <solid android:color="@color/style_color_primary_thin_color" />
      <stroke android:width="2dp"/>
      <size android:width="5dp" android:height="3dp"/>
    </shape>
  </item>
  <item
    android:bottom="16dp"
    android:left="16dp"
    android:right="16dp"
    android:top="16dp">
    <shape android:shape="oval">
      <solid android:color="@color/style_color_primary" />
    </shape>
  </item>
  <item>
    <shape android:shape="oval">
      <stroke
        android:width="2dp"
        android:color="@color/style_color_primary_dark"
        android:dashWidth="0dp" />
    </shape>
  </item>
</layer-list>